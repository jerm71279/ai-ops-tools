{
  "name": "OberaAI Strategy - Daily Aggregation",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [{"field": "cronExpression", "expression": "0 6 * * *"}]
        }
      },
      "id": "schedule-trigger",
      "name": "Daily 6 AM",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "command": "cd ~/oberaconnect-ai-ops/core/OberaAIStrategy && python3 run_strategy.py --json"
      },
      "id": "run-strategy",
      "name": "Run OberaAI Strategy",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [450, 300]
    }
  ],
  "connections": {
    "Daily 6 AM": {"main": [[{"node": "Run OberaAI Strategy", "type": "main", "index": 0}]]}
  },
  "settings": {"executionOrder": "v1"},
  "tags": [{"name": "cron-migration"}, {"name": "daily"}]
}
