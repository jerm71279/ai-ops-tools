{
  "name": "Azure VM Backup - Daily Report",
  "nodes": [
    {
      "parameters": {
        "rule": {
          "interval": [{"field": "cronExpression", "expression": "0 7 * * *"}]
        }
      },
      "id": "schedule-trigger",
      "name": "Daily 7 AM CST",
      "type": "n8n-nodes-base.scheduleTrigger",
      "typeVersion": 1.1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "command": "AZURE_CONFIG_DIR=/home/mavrick/AZ_Subs/customer/setcoservices python3 /home/mavrick/Projects/Secondbrain/scripts/azure_backup_report.py --sharepoint 2>&1"
      },
      "id": "run-backup-report",
      "name": "Generate & Upload Report",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [450, 300]
    }
  ],
  "connections": {
    "Daily 7 AM CST": {"main": [[{"node": "Generate & Upload Report", "type": "main", "index": 0}]]}
  },
  "settings": {"executionOrder": "v1"},
  "tags": [{"name": "daily"}, {"name": "azure"}, {"name": "backup"}, {"name": "sharepoint"}]
}
