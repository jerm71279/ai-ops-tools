        <div class="status-boxes">
            <div class="status-box">
                <input type="checkbox" id="status-bench">
                <label for="status-bench">Bench Tested</label>
            </div>
            <div class="status-box">
                <input type="checkbox" id="status-deployed">
                <label for="status-deployed">Deployed</label>
            </div>
            <div class="status-box">
                <input type="checkbox" id="status-verified">
                <label for="status-verified">Verified</label>
            </div>
            <div class="status-box">
                <input type="checkbox" id="status-complete">
                <label for="status-complete">Complete</label>
            </div>
        </div>

        <div style="margin-top: 20px;">
            <strong>Completed By:</strong> <input type="text" style="width:200px;border:none;border-bottom:1px solid #333;">
            <strong style="margin-left: 30px;">Date:</strong> <input type="text" style="width:150px;border:none;border-bottom:1px solid #333;">
            <strong style="margin-left: 30px;">Time:</strong> <input type="text" style="width:100px;border:none;border-bottom:1px solid #333;">
        </div>

        <footer>
            <p><em>Generated by Multi-Vendor Network Config Builder</em></p>
            <p>Customer: DC Lawn | Circuit ID: 205922 | Location: Fairhope, AL</p>
        </footer>
    </div>
